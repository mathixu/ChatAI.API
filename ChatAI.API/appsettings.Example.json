{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "address": "http://localhost:5000",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=your_db;Uid=user;Pwd=password"
  },
  "Jwt": {
    "Issuer": "your_issuer",
    "Audience": "your_audience",
    "SecretKey": "your_secret_key",
    "ExpireInMinutes": 15
  }
}
