{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "address": "http://localhost:5000",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=your_mysql_db;Uid=user;Pwd=password"
  },
  "Jwt": {
    "Issuer": "your_issuer",
    "Audience": "your_audience",
    "SecretKey": "your_secret_key",
    "AccessTokenExpireInMinutes": 0,
    "ResetPasswordTokenExpireInMinutes": 0
  },
  "RefreshToken": {
    "ExpireInDays": 0
  },
  "Smtp": {
    "Host": "your_smtp_host",
    "Port": 0,
    "Username": "your_smtp_username",
    "Password": "your_smtp_password",
    "SenderEmail": "your_smtp_sender_email",
    "SenderName": "your_smtp_sender_name"
  }
}